<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<OutputType>library</OutputType>
	<PackageId>FreePLM.Common</PackageId>
	<Version>10.0.2</Version>
	<Authors>FreePLM.Common</Authors>
	<Company>FreePLM.Common</Company>
	<PackageDescription>FreePLM.Common</PackageDescription>
	<RepositoryUrl>https://github.com/FreePLM/FreePLM.Common</RepositoryUrl>
	<IsPackable>true</IsPackable>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <!-- Reference FreePLM.Core as project (not NuGet package) -->
    <ProjectReference Include="..\..\FreePLM.Core\FreePLM.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Services\" />
  </ItemGroup>

</Project>
